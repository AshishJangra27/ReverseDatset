   "source": [
    "df = open('NSE-TCS.csv','r')\n",
    "rev = open('Rev.csv','a+')\n",
    "\n",
    "app = []\n",
    "for lines in df:\n",
    "    app.append(lines)\n",
    "\n",
    "rev.write(app[0])\n",
    "\n",
    "for i in range(len(app)-1,0,-1):\n",
    "    rev.write(app[i])\n",
    "    \n",
    "rev.close()\n",
    "df.close()"
   ]
